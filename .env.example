# Application
KIN_ENV=development

# Database
DATABASE_WRITE_URL=postgres://kin:kin_password@localhost:5432/kin?sslmode=disable
DATABASE_READ_URL=postgres://kin:kin_password@localhost:5432/kin?sslmode=disable
POSTGRES_USER=kin
POSTGRES_PASSWORD=kin_password
POSTGRES_DB=kin

# Redis
REDIS_URL=redis://localhost:6379

# S3/MinIO
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET=kin-media
S3_REGION=us-east-1
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin

# Auth0
AUTH0_DOMAIN=your-tenant.auth0.com
AUTH0_AUDIENCE=https://api.kin.app
AUTH0_CLIENT_ID=your-client-id

# Server
SERVER_PORT=8080
SERVER_HOST=0.0.0.0
